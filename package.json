{
  "name": "parts-zone",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.16.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "lint": "next lint",
    "check-app-types": "tsc -p ./tsconfig.json",
    "check-test-types": "tsc -p ./tsconfig.json",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:pull:dev": "npx dotenv -e .env -- npx prisma db pull && npx dotenv -e .env -- npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env -- npx prisma migrate dev --preview-feature",
    "prisma:generate": "dotenv -e .env -- npx prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.409.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.4",
    "@mui/system": "^5.13.2",
    "@mui/x-date-pickers": "^6.12.1",
    "@prisma/client": "^5.20.0",
    "@tanstack/react-query": "^4.35.0",
    "@tanstack/react-query-devtools": "^4.35.0",
    "axios": "^1.7.7",
    "formik": "^2.4.5",
    "luxon": "^3.4.3",
    "next": "^14.2.15",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-phone-number-input": "^3.4.0",
    "react-query": "^3.39.3",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.3",
    "sharp": "^0.33.3",
    "swiper": "^11.0.5",
    "use-debounce": "^10.0.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jest": "^29.5.2",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^9.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^14.2.15",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.3"
  }
